{
  "manifest_version": 3,
  "name": "Web Form Styler and AI Assistant",
  "version": "1.4.0",
  "description": "Automatically style web forms and provide AI assistance",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "https://api.coze.cn/*"
  ],
  "host_permissions": [
    "https://api.coze.cn/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
